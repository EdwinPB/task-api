create_table("tasks") {
	t.Column("id", "uuid", {primary: true})
	t.Column("description", "string", {})
	t.Column("completed", "bool", {default: false})
	t.Column("completion_date", "timestamp", {"null": true})
	t.Column("requester_name", "string", {})
	t.Column("executor_name", "string", {})
	t.Timestamps()
}